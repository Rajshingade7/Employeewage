const arr=[2,34,56,6,7];
const reverse=[];
function reversefunction(callback){
    for(let i=0;i<callback.length;i++){
        reverse.unshift(callback[i]);//adding the element at first position
    }
    console.log(reverse);
    for(let i=callback.length-1; i>0;i--){
        reverse.push(callback[i]);//adding element at last position
    }
}
reversefunction(arr);